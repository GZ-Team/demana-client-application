{
  "extends": "@electron-toolkit/tsconfig/tsconfig.web.json",
  "include": [
    "src/types.ts",
    "src/renderer/*"
  ],
  "compilerOptions": {
    "composite": true,
    "baseUrl": ".",
    "paths": {
      "@ui/*": [
        "./src/renderer/ui/src/*"
      ],
      "@worker/*": [
        "./src/renderer/worker/src/*"
      ]
    }
  }
}
