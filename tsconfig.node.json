{
  "extends": "@electron-toolkit/tsconfig/tsconfig.web.json",
  "include": [
    "src/renderer/ui/src/env.d.ts",
    "src/renderer/ui/src/**/*",
    "src/renderer/ui/src/**/*.vue",
    "src/renderer/ui/src/pages/**/*.vue",
    "src/renderer/ui/src/components/**/*.vue",
    "src/renderer/ui/src/layouts/*.vue"
  ],
  "compilerOptions": {
    "composite": true,
    "baseUrl": ".",
    "paths": {
      "@ui/*": [
        "./src/renderer/ui/src/*"
      ],
      "@worker/*": [
        "./src/renderer/worker/src/*"
      ]
    }
  }
}
